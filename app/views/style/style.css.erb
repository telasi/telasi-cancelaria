* {margin: 0; padding: 0;}
#main {padding: 5px;}
<%= render :partial => 'style/header' %>
<%= render :partial => 'style/footer' %>
<%= render :partial => 'style/action' %>
<%= render :partial => 'style/notice' %>
body {font-size: 14px;}
h1 {font-size: 19px; color: #e5ad17; margin-bottom: 10px;}
table { border: 1px solid #9f1212; border-collapse:collapse; }
table th { background: #f6eccf; border: 1px solid #9f1212; padding: 3px 5px; text-align: center; }
table td { padding: 3px 5px; border: 1px solid #9f1212; }
.empty { color: #777; font-style: italic; padding: 10px;}