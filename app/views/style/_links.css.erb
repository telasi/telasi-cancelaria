a.create { background: url(<%= image_path('16x16/add.png') %>) no-repeat 0px 50%; padding-left: 20px; }
a.edit { background: url(<%= image_path('16x16/pencil.png') %>) no-repeat 0px 50%; padding-left: 20px; }
a.back { background: url(<%= image_path('16x16/arrow_left.png') %>) no-repeat 0px 50%; padding-left: 20px; }
a.delete { background: url(<%= image_path('16x16/delete.png') %>) no-repeat 0px 50%; padding-left: 20px; color: red; }
a.delete:hover { background-color: red; color: white; }
a.exclaim { background: url(<%= image_path('16x16/exclamation.png') %>) no-repeat 0px 50%; padding-left: 20px; }
a.exclaim:hover { background-color: red; color: white; }